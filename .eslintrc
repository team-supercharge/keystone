{
  "extends": [
     "keystone-react"
  ],
  "rules": {
    "react/jsx-no-bind": 0,
    "react/jsx-indent": [2, 4],
    "react/jsx-indent-props": [2, 4],
    "indent-size": 1,
    "indent": [2, 4]
  },
  "globals": {
    "_": true,
    "$": true,
    "after": true,
    "afterEach": true,
    "before": true,
    "beforeEach": true,
    "describe": true,
    "it": true,
    "jQuery": true,
    "Keystone": true
  }
}
